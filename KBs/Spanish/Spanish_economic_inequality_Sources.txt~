(B (name cultivo)(=>(^(S#CULTIVO e0 :0.3)(SS#PLANTING x e0 :0.3)(R#CROP x e0 :0.3)) (crop-nn e0 x)))
(B (name sembrar)(=>(^(S#CROP e0 :0.23)(SS#PLANTING e0 e0 :0.23)(R#GARDENER x e0 :0.23)(R#CROP y e0 :0.23)) (sembrar-vb e0 x y u)))
(B (name plantar)(=>(^(S#CROP e0 :0.23)(SS#PLANTING e0 e0 :0.23)(R#GARDENER x e0 :0.23)(R#CROP y e0 :0.23)) (plantar-vb e0 x y u)))

; CULTIVATION
(B (name crecer)(=>(^(S#CROP e0 :0.23)(SS#CULTIVATION e0 e0 :0.23)(R#GARDENER x e0 :0.23)(R#CROP y e0 :0.23)) (crecer-vb e0 x y u)))

; OUTCOME
(B (name cosechar)(=>(^(S#CROP e0 :0.23)(SS#OUTCOME e0 e0 :0.23)(R#GARDENER x e0 :0.23)(R#CROP x e0 :0.23)) (cosechar-vb e0 x y u)))
(B (name cosecha)(=>(^(S#CROP e0 :0.3)(SS#OUTCOME x e0 :0.3)(R#CROP y e0 :0.3)) (cosecha-nn e0 x)))

(B (name cropmap)(=>(I#CROP x :0.9)(S#CROP x)))
(B (name crop-plantingmap)(=>(I#PLANTING x y :0.9)(SS#PLANTING x y)))
(B (name crop-cultivationmap)(=>(I#CULTIVATION x y :0.9)(SS#CULTIVATION x y)))
(B (name crop-outcomemap)(=>(I#OUTCOME x y :0.9)(SS#OUTCOME x y)))
